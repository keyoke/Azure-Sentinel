{
    "name": "DTAttacks2DCR",
    "apiVersion": "2021-09-01-preview",
    "type": "Microsoft.Insights/dataCollectionRules",
    "location": "{{location}}",
    "properties": {
        "streamDeclarations": {
            "Custom-DynatraceAttacks2": {
                "columns": [
                    {
                        "name": "type",
                        "type": "string"
                    },
                    {
                        "name": "attackId",
                        "type": "string"
                    },
                    {
                        "name": "displayId",
                        "type": "string"
                    },
                    {
                        "name": "timestamp",
                        "type": "string"
                    },
                    {
                        "name": "displayName",
                        "type": "string"
                    },
                    {
                        "name": "attackType",
                        "type": "string"
                    },
                    {
                        "name": "state",
                        "type": "string"
                    }
                ]
            }
        },
        "destinations": {
            "logAnalytics": [
                {
                    "workspaceResourceId": "{{workspaceResourceId}}",
                    "name": "clv2ws1"
                }
            ]
        },
        "dataFlows": [
            {
                "streams": [
                    "Custom-DynatraceAttacks2"
                ],
                "destinations": [
                    "clv2ws1"
                ],
                "transformKql": "source | extend TimeGenerated = now() | project-away type",
                "outputStream": "Custom-DynatraceAttacksV2_CL"
            }
        ],
        "dataCollectionEndpointId": "[concat('/subscriptions/',parameters('subscription'),'/resourceGroups/',parameters('resourceGroupName'),'/providers/Microsoft.Insights/dataCollectionEndpoints/',parameters('workspace'))]"
    }
}